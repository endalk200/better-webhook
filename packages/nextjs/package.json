{
  "name": "@better-webhook/nextjs",
  "version": "0.8.1",
  "description": "Next.js integration for better-webhook",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "tsc --noEmit",
    "check-types": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "clean": "rm -rf dist .turbo",
    "prepublishOnly": "pnpm run build"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "keywords": [
    "webhook",
    "nextjs",
    "next",
    "webhook-handler",
    "api-routes",
    "route-handlers"
  ],
  "license": "MIT",
  "author": "Endalk <endalk200>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/endalk200/better-webhook.git",
    "directory": "packages/nextjs"
  },
  "bugs": {
    "url": "https://github.com/endalk200/better-webhook/issues"
  },
  "homepage": "https://github.com/endalk200/better-webhook#readme",
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "@better-webhook/core": "workspace:*"
  },
  "peerDependencies": {
    "next": "^13.0.0 || ^14.0.0 || ^15.0.0 || ^16.0.0"
  },
  "devDependencies": {
    "@better-webhook/typescript-config": "workspace:*",
    "@types/node": "^24.3.1",
    "next": "^16.1.1",
    "tsup": "^8.5.0",
    "typescript": "^5.6.3",
    "vitest": "^4.0.16",
    "zod": "^4.0.0"
  }
}

{
  "name": "@better-webhook/ragie",
  "version": "0.1.0",
  "description": "Ragie module for better-webhook",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "tsc --noEmit",
    "check-types": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "clean": "rm -rf dist .turbo",
    "prepublishOnly": "pnpm run build"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "keywords": [
    "webhook",
    "ragie",
    "webhook-handler",
    "module",
    "api",
    "rag"
  ],
  "license": "MIT",
  "author": "Endalk <endalk200>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/endalk200/better-webhook.git",
    "directory": "packages/ragie"
  },
  "bugs": {
    "url": "https://github.com/endalk200/better-webhook/issues"
  },
  "homepage": "https://github.com/endalk200/better-webhook#readme",
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "@better-webhook/core": "workspace:*",
    "zod": "^4.0.0"
  },
  "peerDependencies": {},
  "devDependencies": {
    "@better-webhook/typescript-config": "workspace:*",
    "@types/node": "^24.3.1",
    "tsup": "^8.5.0",
    "typescript": "^5.6.3",
    "vitest": "^4.0.16"
  }
}


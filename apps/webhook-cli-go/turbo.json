{
  "$schema": "https://turborepo.com/schema.json",
  "extends": ["//"],
  "tasks": {
    "build": {
      "dependsOn": [],
      "inputs": ["$TURBO_DEFAULT$", "go.mod", "go.sum", "**/*.go"],
      "outputs": ["bin/**"]
    },
    "lint": {
      "dependsOn": [],
      "inputs": ["$TURBO_DEFAULT$", "go.mod", "go.sum", "**/*.go"]
    },
    "check-types": {
      "dependsOn": [],
      "inputs": ["$TURBO_DEFAULT$", "go.mod", "go.sum", "**/*.go"]
    },
    "test": {
      "dependsOn": [],
      "inputs": ["$TURBO_DEFAULT$", "go.mod", "go.sum", "**/*.go"]
    },
    "clean": {
      "cache": false,
      "outputs": ["bin/**"]
    }
  }
}

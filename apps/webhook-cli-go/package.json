{
  "name": "@better-webhook/cli-go",
  "private": true,
  "version": "0.0.0",
  "description": "Local-only Go CLI experiment for better-webhook",
  "scripts": {
    "lint": "go vet ./...",
    "check-types": "go vet ./...",
    "test": "go test ./...",
    "test:race": "go test ./... -race",
    "test:cover": "go test ./... -coverprofile=coverage.out -covermode=atomic",
    "test:e2e": "go test ./e2e -tags=e2e",
    "build": "mkdir -p bin && go build -o bin/better-webhook ./cmd/better-webhook",
    "clean": "rm -rf bin"
  }
}
